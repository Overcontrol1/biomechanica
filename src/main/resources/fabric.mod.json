{
  "schemaVersion": 1,
  "id": "biomechanica",
  "version": "${version}",
  "name": "Biomechanica",
  "description": "Biomechanics takes over the world. Will you become one with the machines, ",
  "authors": [
    "Overcontrol1",
    "Devils"
  ],
  "contact": {
    "repo": "https://github.com/those/Biomechanica"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/biomechanica/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.overcontrol1.biomechanica.client.BiomechanicaClient"
    ],
    "main": [
      "com.overcontrol1.biomechanica.Biomechanica"
    ],
    "cardinal-components": [
      "com.overcontrol1.biomechanica.cca.BiomechanicaEntityComponents",
      "com.overcontrol1.biomechanica.cca.BiomechanicaItemComponents"
    ],
    "fabric-datagen": [
      "com.overcontrol1.biomechanica.datagen.BiomechanicaDatagen"
    ]
  },
  "mixins": [
    "biomechanica.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "geckolib": ">=4.0",
    "minecraft": "${minecraft_version}"
